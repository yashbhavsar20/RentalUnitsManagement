<AnchorPane prefHeight="526.0" prefWidth="735.0" stylesheets="@../../css/home-screen.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.lease.RentUnitController">
    <children>
        <ImageView fitHeight="526.0" fitWidth="743.0" layoutX="-8.0" pickOnBounds="true">
            <image>
                <Image url="@../../assets/082fbeaf4e8047c886e660a26508ef4b-cc_ft_960.jpg" />
            </image>
        </ImageView>
        <Pane layoutX="85.0" layoutY="14.0" prefHeight="480.0" prefWidth="565.0" style="-fx-background-radius: 50; -fx-background-color: rgba(38, 50, 87,0.88);">
            <children>
                <Label layoutX="36.0" layoutY="28.0" style="-fx-font-weight: 700; -fx-font-family: Arial Rounded MT Bold;" stylesheets="@../../css/home-screen.css" text="Rent Unit Page" textFill="WHITE">
                    <font>
                        <Font size="36.0" />
                    </font>
                </Label>
                <TextField fx:id="propertyIdField" layoutX="36.0" layoutY="98.0" promptText="Enter property ID" />
                <TextField fx:id="tenantIdField" layoutX="334.0" layoutY="98.0" promptText="Enter tenant ID" />
                <TextArea fx:id="leaseInfoField" layoutX="36.0" layoutY="145.0" prefHeight="112.0" prefWidth="246.0" promptText="Enter lease info" wrapText="true" />
                <TextField fx:id="leaseStartDateField" layoutX="334.0" layoutY="166.0" promptText="Enter lease start date" />
                <TextField fx:id="leaseEndDateField" layoutX="334.0" layoutY="229.0" promptText="Enter lease end date" />
                <TextField fx:id="rentAmountField" layoutX="208.0" layoutY="284.0" promptText="Enter rent amount" />
                <Button fx:id="submitButton" onAction="#onSubmitButton" layoutX="232.0" layoutY="356.0"  mnemonicParsing="false" text="Submit" />
                <Label fx:id="resLabel" layoutX="331.0" layoutY="436.0" textFill="WHITE" />
            </children>
        </Pane>
        <Hyperlink layoutX="14.0" layoutY="14.0" onAction="#onHomeClick" stylesheets="@../../css/home-screen.css" text="Home"/>
        <!--      <Button layoutX="212.0" layoutY="200.0" mnemonicParsing="false" onAction="#onHomeButton" text="Go to home screen" />-->
    </children>
</AnchorPane>
